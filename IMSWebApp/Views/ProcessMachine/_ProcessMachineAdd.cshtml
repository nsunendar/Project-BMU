<div class="container mt-4">
    <form id="editProcessForm">
        @Html.AntiForgeryToken()

@*        <div class="form-group row">
            <label for="RelasiCode" class="col-sm-2 col-form-label">Relasi Code</label>
            <div class="col-sm-10">
            <input type="text" id="RelasiCode" name="RelasiCode" class="form-control" />
            </div>
        </div> *@

        <div class="form-group row">
            <label for="ProcCode" class="col-sm-2 col-form-label">Process Code</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="ProcCode" name="ProcCode" />
                </div>
        </div>

        <div class="form-group row">
            <label for="NoProcess" class="col-sm-2 col-form-label">Process Number</label>
            <div class="col-sm-10">
                <input type="number" class="form-control" id="NoProcess" name="NoProcess" />
                </div>
        </div>

        <div class="form-group row">
            <label for="CodeMachine" class="col-sm-2 col-form-label">Machine Code</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="CodeMachine" name="CodeMachine" />
                </div>
        </div>

        <div class="form-group row">
            <label for="QtyUsage" class="col-sm-2 col-form-label">Quantity Usage</label>
            <div class="col-sm-10">
                <input type="number" step="0.0001" class="form-control" id="QtyUsage" name="QtyUsage" />
                </div>
        </div>

        <div class="form-group row">
            <label for="TarProDay" class="col-sm-2 col-form-label">Tariff per Day</label>
            <div class="col-sm-10">
                <input type="number" step="0.0001" class="form-control" id="TarProDay" name="TarProDay" />
                </div>
        </div>

        <div class="form-group row">
            <label for="TarProHours" class="col-sm-2 col-form-label">Tariff per Hour</label>
            <div class="col-sm-10">
                <input type="number" step="0.0001" class="form-control" id="TarProHours" name="TarProHours" />
                </div>
        </div>

        <div class="form-group row">
            <label for="DateProcess" class="col-sm-2 col-form-label">Date Process</label>
            <div class="col-sm-10">
                <input type="datetime-local" class="form-control" id="DateProcess" name="DateProcess" />
                </div>
        </div>

        <button type="submit" class="btn btn-primary">Save</button>
   @*      <button type="button" class="btn btn-secondary" onclick="cancelEdit()">Cancel</button> *@
    </form>
</div>

<script src="~/js/processmachine.js"></script>